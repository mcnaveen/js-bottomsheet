{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": [".eslintcache"]
    },
    "lint:fix": {
      "outputs": [".eslintcache"]
    },
    "lintstyles": {
      "outputs": [".stylelintcache"]
    },
    "lintstyles:fix": {
      "outputs": [".stylelintcache"]
    },
    "format": {},
    "format:fix": {},
    "test": {},
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist"]
    }
  }
}
